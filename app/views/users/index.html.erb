<div class="usersbody">
  <div class="blogeachtitle">
    <p2>会員一覧</p2>
  </div>
  <table class="table table-hover">
    <tr>
      <th>名前</th>
      <th>Email</th>
      <th>管理者</th>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td>
          <% if user.admin %>
            <%= '🐓' %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
