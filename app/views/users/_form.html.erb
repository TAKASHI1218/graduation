<%= form_with(model: @user, local: true) do |form| %>

  <div class="user_name">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="user_email">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <%= form.submit %>
<% end %>

<%= link_to "ユーザー一覧画面に戻る", users_path %>
